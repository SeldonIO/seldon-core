{
	"apiVersion": "machinelearning.seldon.io/v1alpha2",
	"kind": "SeldonDeployment",
	"metadata": {
		"name": "seldon-model"
	},
	"spec": {
		"name": "test-deployment",
		"oauth_key": "oauth-key",
		"oauth_secret": "oauth-secret",
		"predictors": [
			{
				"componentSpecs": [
					{
						"metadata": {
							"name": "my-dep"
						},
						"spec": {
							"containers": [
								{
									"image": "seldonio/mock_classifier:1.0",
									"imagePullPolicy": "IfNotPresent",
									"name": "classifier",
									"resources": {
										"requests": {
											"cpu": "0.5"
										}
									}
								}
							],
							"terminationGracePeriodSeconds": 1
						},
						"hpaSpec": {
							"minReplicas": 1,
							"maxReplicas": 4,
							"metrics": [
								{
									"type": "Resource",
									"resource": {
										"name": "cpu",
										"targetAverageUtilization": 10
									}
								}
							]
						}
					}
				],
				"graph": {
					"children": [
					],
					"name": "classifier",
					"endpoint": {
						"type": "REST"
					},
					"type": "MODEL"
				},
				"name": "example",
				"replicas": 1
			}
		]
	}
}