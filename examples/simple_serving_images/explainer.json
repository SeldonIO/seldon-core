{
  "apiVersion": "machinelearning.seldon.io/v1alpha2",
  "kind": "SeldonDeployment",
  "metadata": {
    "labels": {
      "app": "seldon"
    },
    "name": "income-classifier"
  },
  "spec": {
    "name": "income-classifier",
    "predictors": [
      {
        "componentSpecs": [{
          "spec": {
            "containers": [
              {
                "image": "seldonio/mock_income_classifier:0.2",
                "imagePullPolicy": "IfNotPresent",
                "name": "income-classifier"
              }
            ],
            "terminationGracePeriodSeconds": 20
          }
        }],
        "graph": {
          "children": [],
          "name": "income-classifier",
          "type": "MODEL",
          "endpoint": {
            "type": "REST"
          },
          "parameters": [
            {
              "name": "model_filename",
              "value": "model.joblib",
              "type": "STRING"

            }
          ]
        },
        "name": "income-classifier",
        "replicas": 1,
        "explainer": {
          "type": "ANCHOR_TABULAR",
          "modelUri": "gs://seldon-models/sklearn/income/explainer"
        }
      }
    ]
  }
}
