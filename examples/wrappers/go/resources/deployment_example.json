{
    "apiVersion": "machinelearning.seldon.io/v1alpha2",
    "kind": "SeldonDeployment",
    "metadata": {
        "labels": {
            "app": "seldon"
        },
        "name": "example-go"
    },
    "spec": {
        "name": "go-deployment",
        "oauth_key": "oauth-key",
        "oauth_secret": "oauth-secret",
        "predictors": [
            {
                "componentSpecs": [{
                    "spec": {
                        "containers": [
                            {
                                "image": "seldonio/gomodel:0.1",
                                "imagePullPolicy": "IfNotPresent",
                                "name": "gomodel",
				"env": [
				    {
					"name": "SERVER_TYPE",
					"value": "rest"
				    }
				]
                            }
                        ],
                        "terminationGracePeriodSeconds": 1
                    }
                }],
                "graph": {
                    "children": [],
                    "name": "gomodel",
                    "endpoint": {
                        "type" : "REST"
                    },
                    "type": "MODEL"
                },
                "name": "go-predictor",
                "replicas": 1
            }
        ]
    }
}
