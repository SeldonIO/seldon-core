{
    "apiVersion": "machinelearning.seldon.io/v1alpha2",
    "kind": "SeldonDeployment",
    "metadata": {
    "labels": {
        "app": "seldon"
    },
    "name": "ts-deployment"
    },
    "spec": {
    "annotations": {
        "project_name": "Thompson Sampling Routing",
        "deployment_version": "v1"
    },
    "name": "poc-ts",
    "oauth_key": "oauth-key",
    "oauth_secret": "oauth-secret",
    "predictors": [
        {
        "componentSpecs": [{
            "spec": {
            "containers": [
                {
                "image": "seldonio/rf_model:1.0",
                "name": "rf-model"
                },
                {
                "image": "seldonio/xgb_model:1.0",
                "name": "xgb-model"
                },
                {
                "image": "seldonio/mab_ts:1.0",
                "name": "ts-router"
                }
            ],
            "terminationGracePeriodSeconds": 20
            }
        }],
        "name": "ts-2",
        "replicas": 1,
        "annotations": {
            "predictor_version": "v1"
        },
        "graph": {
            "name": "ts-router",
            "type":"ROUTER",
            "parameters": [
            {
                "name": "n_branches",
                "value": "2",
                "type": "INT"
            },
            {
                "name": "verbose",
                "value": "1",
                "type": "BOOL"
            },
            {
                "name": "branch_names",
                "value": "rf:xgb",
                "type": "STRING"
            },
            {
                "name":"seed",
                "value":"1",
                "type":"INT"
            }
            ],
            "children": [
            {
                "name": "rf-model",
                "type": "MODEL"
            },
            {
                "name": "xgb-model",
                "type": "MODEL"
            }
            ]
        }
        }
    ]
    }
}

