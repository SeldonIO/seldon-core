{
  "apiVersion": "machinelearning.seldon.io/v1",
  "kind": "SeldonDeployment",
  "metadata": {
    "labels": {
      "app": "seldon"
    },
    "name": "rf-deployment"
  },
  "spec": {
    "annotations": {
      "project_name": "Random Forest Model",
      "deployment_version": "v1"
    },
    "name": "rf-model",
    "predictors": [
      {
        "componentSpecs": [
          {
            "spec": {
              "containers": [
                {
                  "image": "seldonio/credit_default_rf_model:0.2",
                  "name": "rf-model"
                }
              ],
              "terminationGracePeriodSeconds": 20
            }
          }
        ],
        "name": "rf-model",
        "replicas": 1,
        "annotations": {
          "predictor_version": "v1"
        },
        "graph": {
          "name": "rf-model",
          "type": "MODEL",
          "children": []
        }
      }
    ]
  }
}
