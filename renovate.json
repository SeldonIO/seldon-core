{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "Standard renovate config",

  "extends": [
    "github>cerebrotech/renovate-config:docker-minimal"
  ], 
 
  "packageRules": [
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackagePatterns": [
        "quay.io/domino/distroless.base-debian11"
      ],
      "pinDigests": true,
      "versioning": "docker"
    }
  ]
}
