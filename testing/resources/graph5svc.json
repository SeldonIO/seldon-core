{
  "apiVersion": "machinelearning.seldon.io/v1alpha2",
  "kind": "SeldonDeployment",
  "metadata": {
    "labels": {
      "app": "seldon"
    },
    "name": "mymodel"
  },
  "spec": {
    "annotations": {
      "seldon.io/engine-separate-pod": "true"
    },
    "name": "mymodel",
    "predictors": [
      {
        "componentSpecs": [
          {
            "spec": {
              "containers": [
                {
                  "image": "seldonio/fixed-model:0.1",
                  "imagePullPolicy": "IfNotPresent",
                  "name": "model1",
                  "resources": {
                    "requests": {
                      "memory": "1Mi"
                    }
                  }
                }
              ],
              "terminationGracePeriodSeconds": 1
            }
          },
          {
            "spec": {
              "containers": [
                {
                  "image": "seldonio/fixed-model:0.1",
                  "imagePullPolicy": "IfNotPresent",
                  "name": "model2",
                  "resources": {
                    "requests": {
                      "memory": "1Mi"
                    }
                  }
                }
              ],
              "terminationGracePeriodSeconds": 1
            }
          }
        ],
        "graph": {
          "name": "model1",
          "endpoint": {
            "type": "REST"
          },
          "type": "MODEL",
          "children": [
            {
              "children": [],
              "name": "model2",
              "endpoint": {
                "type": "REST"
              },
              "type": "MODEL"
            }
          ]
        },
        "name": "mymodel",
        "replicas": 1
      }
    ]
  }
}
