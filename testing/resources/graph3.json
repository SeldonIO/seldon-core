{
    "apiVersion": "machinelearning.seldon.io/v1alpha2",
    "kind": "SeldonDeployment",
    "metadata": {
        "labels": {
            "app": "seldon"
        },
        "name": "mymodel"
    },
    "spec": {
        "name": "mymodel",
        "annotations": {
            "seldon.io/ambassador-retries": "3",
            "seldon.io/istio-retries": "3"
        },
        "predictors": [
            {
                "componentSpecs": [{
                    "spec": {
                        "containers": [
                            {
                                "image": "seldonio/fixed-model:0.2",
                                "imagePullPolicy": "IfNotPresent",
                                "name": "complex-model2",
                                "resources": {
                                    "requests": {
                                        "memory": "1Mi"
                                    }
                                }
                            }
                        ],
                        "terminationGracePeriodSeconds": 1
                    }
                }],
                "graph": {
                    "children": [],
                    "name": "complex-model2",
                    "endpoint": {
                        "type" : "REST"
                    },
		    "type": "MODEL"
                },
                "name": "mymodel",
                "replicas": 1
            }
        ]
    }
}
